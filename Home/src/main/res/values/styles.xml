<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar" tools:ignore="ResourceName">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="home_style_home_head_icon">
        <item name="android:layout_width">@dimen/dp_42</item>
        <item name="android:layout_height">@dimen/dp_42</item>
    </style>

    <style name="home_style_home_head_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dp_4</item>
        <item name="android:textColor">@color/common_black</item>
        <item name="android:textSize">@dimen/sp_14</item>
    </style>

    <style name="home_style_home_head_llay">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/dp_10</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
    </style>

</resources>
